unit Produto;

interface

uses
  Produto.Interfaces;

type
  TProduto = class(TInterfacedObject, iProduto)
    private
    public
      constructor Create;
      destructor Destroy; override;
      class function New : iProduto;
  end;

implementation

{ TProduto }

constructor TProduto.Create;
begin

end;

destructor TProduto.Destroy;
begin

  inherited;
end;

class function TProduto.New: iProduto;
begin
  Result := Self.Create;
end;

end.
